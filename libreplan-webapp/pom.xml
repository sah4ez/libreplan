                                    </tasks>
                                </configuration>
                            </execution>

                            <execution>
                                <id>user-doc-clean</id>
                                <goals>
                                    <goal>run</goal>
                                </goals>
                                <phase>clean</phase>
                                <inherited>false</inherited>
                                <configuration>
                                    <tasks>
                                        <exec executable="make" failifexecutionfails="false">
