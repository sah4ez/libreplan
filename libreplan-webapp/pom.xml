                                        </copy>
                                        <copy todir="src/main/webapp/help/zh" failonerror="false">
                                            <fileset dir="../doc/src/user/en/html"/>
                                        </copy>
                                        <copy todir="src/main/webapp/help/nb" failonerror="false">
                                            <fileset dir="../doc/src/user/en/html"/>
                                        </copy>
                                        <copy todir="src/main/webapp/help/fa_IR" failonerror="false">
