                                        </copy>
                                        <copy todir="src/main/webapp/help/de" failonerror="false">
                                            <fileset dir="../doc/src/user/en/html"/>
                                        </copy>
                                        <copy todir="src/main/webapp/help/ca" failonerror="false">
